    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev:ssr": "ng run rpk-client:serve-ssr",
    "build:ssr": "ng build --prod && ng run rpk-client:server:production",
    "serve:ssr": "node dist/apps/rpk-client/server/main.js",
    "local:ssr": "npm run build:ssr && npm run serve:ssr",
    "prerender": "ng run rpk-client:prerender",
    "docker:build:rpk-client": "docker build -t rpk-client . --build-arg APP_NAME=rpk-client -f .docker/ssr.Dockerfile",
    "docker:run:rpk-client": "docker run -d -it -p 4000:4000 rpk-client",
    "docker:build:rpk-api": "docker build -t rpk-api . --build-arg APP_NAME=rpk-api -f .docker/api.Dockerfile",
    "docker:run:rpk-api": "docker run -d -it -p 3333:3333 rpk-api",
    "docker:start:rpk-client": "npm run docker:build:rpk-client && npm run docker:run:rpk-client",
    "docker:start:rpk-api": "npm run docker:build:rpk-api && npm run docker:run:rpk-api"
  },
